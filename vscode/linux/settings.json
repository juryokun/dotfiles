{
    "window.zoomLevel": 0,
    "debug.allowBreakpointsEverywhere": true,
    "terminal.integrated.fontFamily": "Source Code Pro for Powerline",
    "terminal.integrated.fontSize": 13,
    "vim.easymotion": true,
    "editor.formatOnType": true,
    "editor.formatOnSave": true,
    "vim.whichwrap": "h,l,<,>,[,]",
    "vim.leader": "<Space>",
    "vim.useSystemClipboard": true,
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": true,
    "workbench.colorTheme": "Monokai Dimmed",
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.insert": "#5f8787",
    "vim.statusBarColors.normal": "#294372",
    "vim.statusBarColors.visual": "#af5f87",
    "vim.statusBarColors.visualblock": "#af5f87",
    "vim.statusBarColors.visualline": "#af5f87",
    "vim.statusBarColors.replace": "#5f0000",
    "workbench.colorCustomizations": {
        "statusBar.background": "#294372",
        "statusBar.noFolderBackground": "#294372",
        "statusBar.debuggingBackground": "#294372"
    },
    "editor.wordWrap": "on",
    "editor.minimap.enabled": false,
}