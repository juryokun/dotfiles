{
    /* 共通設定 */
    "window.zoomLevel": 0,
    "debug.allowBreakpointsEverywhere": true,
    "editor.fontFamily": "PlemolJP",
    "editor.fontSize": 14,
    "terminal.integrated.fontFamily": "PlemolJP Console NF",
    "terminal.integrated.fontSize": 13,
    "vim.easymotion": false,
    "vim.sneak": true,
    "editor.formatOnType": true,
    "editor.formatOnSave": true,
    "vim.whichwrap": "h,l,<,>,[,]",
    "vim.leader": "<Space>",
    "vim.useSystemClipboard": true,
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": true,
    "workbench.colorTheme": "Monokai Dimmed",
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.insert": "#5f8787",
    "vim.statusBarColors.normal": "#294372",
    "vim.statusBarColors.visual": "#af5f87",
    "vim.statusBarColors.visualblock": "#af5f87",
    "vim.statusBarColors.visualline": "#af5f87",
    "vim.statusBarColors.replace": "#5f0000",
    "workbench.colorCustomizations": {
        "statusBar.background": "#294372",
        "statusBar.noFolderBackground": "#294372",
        "statusBar.debuggingBackground": "#294372"
    },
    "editor.wordWrap": "on",
    "editor.minimap.enabled": false,
    "editor.tabSize": 4,
    "explorer.openEditors.visible": 0,
    "workbench.editor.openPositioning": "first",
    "workbench.editor.tabSizing": "shrink",
    "files.autoGuessEncoding": true,
    "workbench.editor.enablePreview": false,
    "files.trimTrailingWhitespace": true,
    "editor.renderWhitespace": "all",
    "explorer.autoReveal": false,
    "vim.cursorStylePerMode.visual": "block",
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "ctrl+j"
            ],
            "after": [
                "4",
                "j",
            ]
        },
        {
            "before": [
                "ctrl+k"
            ],
            "after": [
                "4",
                "k",
            ]
        },
        {
            "before": [
                "u"
            ],
            "commands": [
                "undo"
            ]
        },
        {
            "before": [
                "<C-r>"
            ],
            "commands": [
                "redo"
            ]
        }
    ],
    "editor.lineNumbers": "relative",
    "workbench.startupEditor": "none",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.tree.indent": 20,
    "workbench.tree.renderIndentGuides": "always",
    "editor.showFoldingControls": "always",
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    /* 個別設定 */
    "python.venvFolders": [
        "pyvenv"
    ],
    "python.venvPath": "~/pyvenv",
    "rust-client.enableMultiProjectSetup": null,
    "html.format.enable": false,
    "vsnotes.defaultNotePath": "/Users/juryokun/Documents/20.vscode/vsnotes",
    "vsnotes.noteTitleConvertSpaces": " ",
    "vsnotes.defaultNoteTitle": "{dt}_{title}.{ext}",
    "vsnotes.tokens": [
        {
            "type": "datetime",
            "token": "{dt}",
            "format": "YYYY-MM-DD",
            "description": "Insert formatted datetime."
        },
        {
            "type": "title",
            "token": "{title}",
            "description": "Insert note title from input box.",
            "format": "Untitled"
        },
        {
            "type": "extension",
            "token": "{ext}",
            "description": "Insert file extension.",
            "format": "md"
        }
    ],
}